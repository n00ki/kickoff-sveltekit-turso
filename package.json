{
  "type": "module",
  "name": "kickoff-sveltekit-turso",
  "version": "2.0.0",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "db:generate": "drizzle-kit generate:sqlite",
    "db:migrate": "vite-node --options.transformMode.ssr='/.*/' src/lib/db/migrate",
    "db:push": "drizzle-kit push:sqlite",
    "db:studio": "drizzle-kit studio",
    "test": "pnpm test:integration && pnpm test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin prettier-plugin-svelte . --check . && eslint .",
    "format": "prettier --write  --plugin prettier-plugin-svelte .",
    "test:integration": "playwright test",
    "test:unit": "vitest"
  },
  "devDependencies": {
    "@playwright/test": "^1.39.0",
    "@sendgrid/mail": "^7.7.0",
    "@sveltejs/adapter-auto": "^2.1.1",
    "@sveltejs/kit": "^1.27.5",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "autoprefixer": "^10.4.16",
    "bits-ui": "^0.9.4",
    "clsx": "^2.0.0",
    "cssnano": "^6.0.1",
    "dotenv": "^16.3.1",
    "drizzle-kit": "^0.20.1",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.35.0",
    "formsnap": "^0.4.1",
    "postcss": "^8.4.31",
    "postcss-load-config": "^4.0.1",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "radix-icons-svelte": "^1.2.1",
    "svelte": "^4.2.3",
    "svelte-check": "^3.6.0",
    "svelte-french-toast": "^1.2.0",
    "sveltekit-superforms": "^1.10.1",
    "tailwind-merge": "^2.0.0",
    "tailwind-variants": "^0.1.18",
    "tailwindcss": "^3.3.5",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-node": "^0.34.6",
    "vitest": "^0.34.6",
    "zod": "^3.22.4"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.449.0",
    "@aws-sdk/s3-request-presigner": "^3.449.0",
    "@libsql/client": "^0.3.6",
    "@lucia-auth/adapter-sqlite": "^2.0.1",
    "drizzle-orm": "^0.29.0",
    "lucia": "^2.7.4",
    "svelte-email": "^0.0.4",
    "sveltekit-flash-message": "^2.2.1"
  }
}
